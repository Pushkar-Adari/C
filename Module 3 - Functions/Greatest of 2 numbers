#include <stdio.h>
int great (int,int);
void main()
{
    int a,b;
    printf ("Enter the first number: ");
    scanf ("%d",&a);
    printf ("Enter the second number: ");
    scanf ("%d",&b);
    great (a,b);
}
int great (int a,int b)
{
    if (a>b)
    {
        printf ("A is bigger");
    }
    else
    {
        printf ("B is bigger");
    }
}
